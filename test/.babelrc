{
  "presets": [
    ["env", { "modules": "umd" }],
    "stage-2",
    "es2015"
  ],
  "plugins": ["transform-runtime","transform-vue-jsx",["component", [
    {
      "libraryName": "element-ui",
      "styleLibraryName": "theme-chalk"
    }
  ]]],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}