{
    "name": "nutter-ui",
    "description": "nutter's element ui",
    "authors": "nutter",
    "version": "1.0.5",
    "main": "dist/main.umd.js",
    "module": "dist/main.esm.js",
    "unpkg": "dist/main.min.js",
    "keywords": [
        "tools",
        "javascript",
        "library"
    ],
    "dependencies": {
        "axios": "^0.19.2",
        "element-ui": "^2.13.0",
        "vue": "^2.6.11",
        "vue-cookie": "^1.1.4"
    },
    "peerDependencies": {},
    "scripts": {
        "build": "npm run build:umd && npm run build:css",
        "build:browser": "cross-env NODE_ENV=production rollup --config build/rollup.config.browser.js",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.es.js",
        "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.umd.js",
        "build:test": "cross-env NODE_ENV=development rollup --config build/rollup.config.umd.js --watch",
        "build:css": "cross-env NODE_ENV=production rollup --config build/rollup.config.css.js"
    },
    "files": [
        "dist",
        "examples",
        "packages",
        "public"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nutter123/nutter-ui"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.7",
        "@babel/node": "^7.8.7",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/polyfill": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "babel-plugin-import": "^1.13.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "clean-css": "^4.2.3",
        "commitizen": "^4.0.3",
        "conventional-changelog": "^3.1.18",
        "core-js": "^3.6.4",
        "cz-conventional-changelog": "^3.1.0",
        "docz": "^2.2.0",
        "fs": "0.0.1-security",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-css-only": "^2.0.0",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-sass": "^1.2.2",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "rollup-plugin-vue": "^5.1.6",
        "@rollup/plugin-alias": "^3.0.1",
        "vue-template-compiler": "^2.6.11",
        "extend": "^3.0.2",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-require-context": "^1.0.0"
    }
}
